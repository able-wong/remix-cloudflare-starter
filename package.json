{
  "name": "remix-cloudflare-starter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm test && npm run typecheck && npm run lint && npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm test && npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "format": "prettier --write \"app/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.16.6",
    "@remix-run/cloudflare-pages": "^2.16.6",
    "@remix-run/react": "^2.16.6",
    "firebase": "^11.8.1",
    "isbot": "^4.1.0",
    "pino": "^9.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250528.0",
    "@jest/globals": "^29.7.0",
    "@remix-run/dev": "^2.16.6",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.23",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.0.40",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "postcss-nested": "^7.0.2",
    "prettier": "^2.8.8",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^4.17.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/app/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  }
}
